m1=int(input("enter mark in 1st subject:"))
m2=int(input("enter mark in 1st subject:"))
avg=(m1+m2)/2
if avg>80:
     print("grade A")

elif avg>=70 and avg<80:
    print("grade B")

elif avg>=60 and avg<70:
    print("grade C")

elif avg>=50 and avg<60:
    print("grade D")

els: 

    print("grade E")
